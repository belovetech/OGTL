<%= form_with model: [@user, @user.projects.build] do |f| %>
    <div>
        <%= f.label :name %></br>
        <%= f.text_field :name %>
    </div>
    <div>
        <%= f.label :description %></br>
        <%= f.text_area :description %>
    </div>
    <div>
        <%= f.submit  %>
    </div>
<% end %>
